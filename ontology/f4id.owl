        <owl:versionInfo rdf:datatype="http://www.w3.org/2001/XMLSchema#decimal">1.0</owl:versionInfo>
        <rdfs:comment xml:lang="en">An ontology to describe gladiators to integrate Trismegistos/SNAP data model.</rdfs:comment>
    </owl:Ontology>
    


